<script>
  const BASE_URL = 'https://api-football-v1.p.rapidapi.com/v3'
  const API_KEY = '455d68b50dmsh0af0a6836873b4bp178cc7jsn0bb76fff3151'

  const getDataTable = async () => {
    const response = await fetch(`${BASE_URL}/standings/?league=281&season=2025`, {
      headers: {
        'X-RapidAPI-Key': API_KEY,
      },
    })
    const data = await response.json()
    console.log(data.response[0].league.standings[1])
    return data.response[0].league.standings[1]
  }

  const pad = (num) => num.toString().padStart(2, "0"); // ejemplo de pad

  const templateItem = (team) => {
    let logo = team.team.logo;
    let name = team.team.name;

    // Reemplazo de logos según condiciones
    if (team.team.name === "Deportivo Garcilaso") {
      logo = "https://www.claro.com.pe/assets/havas/landing-deportes/liga1/assets/teams/garcilaso.png";
    } else if (team.team.name === "Sport Huancayo") {
      logo = "https://www.claro.com.pe/assets/havas/landing-deportes/liga1/assets/teams/sporthuancayo.png";
    } else if (team.team.name === "Cultural Santa Rosa") {
      logo = "https://www.claro.com.pe/assets/havas/landing-deportes/liga1/assets/teams/chankas.png";
      name = "CHANKAS CYC";
    } else if (team.name === "UTC") {
      name = "UTC Cajamarca";
    }

    return `
      <div class="table__item">
        <ul>
          <li>
            <img src="${logo}" alt="${name}" loading="lazy" width="auto" height="auto" />
            <span>${name}</span>
          </li>
          <li>${pad(team.all.played)}</li>
          <li>${pad(team.all.win)}</li>
          <li>${pad(team.all.draw)}</li>
          <li>${pad(team.all.lose)}</li>
          <li>${pad(team.all.goals.for)}</li>
          <li>${pad(team.all.goals.against)}</li>
          <li>${pad(team.all.goals.for - team.all.goals.against)}</li>
          <li><strong>${pad(team.points)}</strong></li>
        </ul>
      </div>
    `;
  };

  const dataTable = await getDataTable()
  let tableItems = ""
  if (dataTable) {
    dataTable.forEach(team => {
      tableItems += templateItem(team)
    })
  }
  document.querySelector(".table__content").innerHTML = tableItems


  const btn__action = document.querySelector('.btn__more[data-btnaction]');
  const content__action = document.querySelector('.portlet__main__table .table__content')

  btn__action.addEventListener('click', () => {
    content__action.classList.toggle('active')
    btn__action.innerHTML = content__action.classList.contains('active') ? 'Ver menos' : 'Ver más'
  })

</script>
<style>
  .contentTabs {
    max-width: 435px;
    width: 100%;
    display: flex;
    background-color: #fff;
    border-radius: 28.5px;
    margin: 0 auto;
    box-shadow: 0 0 8px 0 rgba(0, 0, 0, 0.25);
  }
  .contentTabs .tab {
    width: 50%;
    color: #da291c;
    text-align: center;
    font-family: Roboto;
    font-size: 16px;
    font-weight: 700;
    line-height: normal;
    border-radius: 28.5px;
    padding: 11px 0;
    cursor: pointer;
  }
  .contentTabs .tab.active {
    color: #fff;
    background-color: #da291c;
  }
  .contentInfoTabs {
    border-radius: 10px;
    background: #fff;
    box-shadow: 0 0 4px 0 rgba(164, 164, 164, 0.25);
    padding: 24px 26px;
    width: fit-content;
    margin: 0 auto;
  }
  .contentInfoTabs .content {
    display: none;
  }
  .contentInfoTabs .content.active {
    display: flex;
    gap: 50px;
    /*display: grid;
    grid-template-columns: repeat(2, 1fr);*/
  }
  .contentInfoTabs .content div {
    display: flex;
    gap: 20px;
    color: #000;
    font-family: AMX;
    font-size: 14px;
    font-weight: 400;
    line-height: normal;
    width: 250px;
  }
  .contentInfoTabs .content div svg {
    flex-shrink: 0;
  }
  @media (max-width: 1023px) {
    .contentTabs {
      margin-top: 10px;
    }
    .contentInfoTabs {
      max-width: 630px;
      width: 100%;
      box-sizing: border-box;
    }
    .contentInfoTabs .content.active {
      flex-direction: column;
      gap: 10px;
    }
    .contentInfoTabs .content div {
      width: 100%;
    }
  }
  /*section.c08 {
    display: none;
  }*/
  section.c08.pB30 {
    display: block;
  }
</style>

<script type="module">
  document.addEventListener("DOMContentLoaded", () => {
    const s = document.querySelectorAll(".contentTabs .tab"),
      l = document.querySelectorAll(".contentInfoTabs .content"),
      i = (t) => {
        setTimeout(() => {
          const e = document.querySelector("section.c08.pB30"),
            o = document.querySelector("section.c08.pT30"),
            a = document.querySelectorAll("section.c08");
          (t === "SOY POSTPAGO" &&
            (a.forEach((c) => c.classList.add("ocultarPlanes")),
            e && e.classList.remove("ocultarPlanes")),
            t === "SOY HOGAR" &&
              (a.forEach((c) => c.classList.add("ocultarPlanes")),
              o && o.classList.remove("ocultarPlanes")));
        }, 300);
      };
    s.forEach((t) => {
      t.addEventListener("click", () => {
        const e = t.getAttribute("cat");
        (s.forEach((c) => c.classList.remove("active")),
          t.classList.add("active"),
          l.forEach((c) => c.classList.remove("active")));
        const o = document.querySelector(
          `.contentInfoTabs .content[cat="${e}"]`,
        );
        (o && o.classList.add("active"), i(e));
        const a = new CustomEvent("cambiarTutorialL1Max", {
          detail: {
            tipo: e === "SOY POSTPAGO" ? "L1MAX MOVIL" : "L1 MAX",
          },
          bubbles: !0,
        });
        document.dispatchEvent(a);
      });
    });
    const n = document.querySelector(".contentTabs .tab.active");
    if ((n && i(n.getAttribute("cat")), n)) {
      const t = n.getAttribute("cat"),
        e = new CustomEvent("cambiarTutorialL1Max", {
          detail: {
            tipo: t === "SOY POSTPAGO" ? "L1MAX MOVIL" : "L1 MAX",
          },
          bubbles: !0,
        });
      setTimeout(() => {
        document.dispatchEvent(e);
      }, 500);
    }


const query = window.location.search.replace("?", "").replace(/\/$/, "").toLowerCase();

      const secTabs = document.querySelector(".secTabs");
      if (!secTabs) return;

      const scrollToTabs = () => {
        const y =
          secTabs.getBoundingClientRect().top +
          window.pageYOffset -
          100;

        window.scrollTo({
          top: y,
          behavior: "smooth"
        });
      };

      const clickTab = (selector) => {
        const tab = document.querySelector(selector);
        if (tab) tab.click();
      };

      if (query === "=movil") {
        scrollToTabs();
        setTimeout(() => {
          clickTab('.tab[cat="SOY POSTPAGO"]');
        }, 500);
      }

      if (query === "=hogar") {
        scrollToTabs();
        setTimeout(() => {
          clickTab('.tab[cat="SOY HOGAR"]');
        }, 500);
      }
  });
</script>
